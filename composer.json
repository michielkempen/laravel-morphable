{
    "name": "michielkempen/laravel-morphable",
    "keywords": [
        "michielkempen",
        "laravel-morphable"
    ],
    "homepage": "https://github.com/michielkempen/laravel-morphable",
    "license": "MIT",
    "authors": [
        {
            "name": "Michiel Kempen",
            "email": "kempenmichiel@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "MichielKempen\\LaravelMorphable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichielKempen\\LaravelMorphable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
